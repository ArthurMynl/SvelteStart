{
	"name": "svelte-template",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"generate-migrations": "drizzle-kit generate --config=src/lib/drizzle/mysql/drizzle.config.ts",
		"push": "drizzle-kit push --config=src/lib/drizzle/mysql/drizzle.config.ts",
		"studio": "drizzle-kit studio --config=src/lib/drizzle/mysql/drizzle.config.ts",
		"migrate": "tsx src/lib/drizzle/mysql/migrations/migrate.ts"
	},
	"devDependencies": {
		"@melt-ui/pp": "^0.1.4",
		"@stripe/stripe-js": "^3.4.0",
		"@sveltejs/adapter-auto": "^3.2.0",
		"@sveltejs/kit": "^2.5.7",
		"@sveltejs/vite-plugin-svelte": "^3.1.0",
		"@tailwindcss/typography": "^0.5.13",
		"@types/jquery": "^3.5.30",
		"@types/node-cron": "^3.0.11",
		"@types/pg": "^8.11.5",
		"@typescript-eslint/eslint-plugin": "^5.62.0",
		"@typescript-eslint/parser": "^5.62.0",
		"autoprefixer": "^10.4.19",
		"daisyui": "^4.10.2",
		"drizzle-kit": "^0.22.1",
		"emailjs": "^4.0.3",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^8.10.0",
		"eslint-plugin-svelte": "^2.38.0",
		"postcss": "^8.4.38",
		"postcss-load-config": "^4.0.2",
		"prettier": "^3.0",
		"prettier-plugin-svelte": "^3.0",
		"prettier-plugin-tailwindcss": "^0.4",
		"svelte": "^4.2.15",
		"svelte-check": "^3.7.0",
		"svelte-headless-table": "^0.18.2",
		"svelte-sequential-preprocessor": "^2.0.1",
		"svelte-stripe": "^1.2.0",
		"sveltekit-superforms": "^2.15.1",
		"tailwindcss": "^3.4.3",
		"tslib": "^2.6.2",
		"tsx": "^3.14.0",
		"typescript": "^5.4.5",
		"vite": "^5.2.10"
	},
	"type": "module",
	"dependencies": {
		"@internationalized/date": "^3.5.4",
		"@libsql/client": "^0.3.6",
		"@lucia-auth/adapter-mysql": "^3.0.2",
		"@melt-ui/svelte": "^0.49.1",
		"arctic": "^1.7.0",
		"bits-ui": "^0.21.11",
		"clsx": "^2.1.1",
		"cmdk-sv": "^0.0.17",
		"dotenv": "^16.4.5",
		"drizzle-orm": "^0.31.0",
		"embla-carousel-svelte": "^8.1.6",
		"formsnap": "^1.0.1",
		"lucia": "^3.2.0",
		"lucide-svelte": "^0.358.0",
		"mode-watcher": "^0.3.1",
		"mysql2": "^3.9.7",
		"octokit": "^3.2.0",
		"oslo": "^1.2.0",
		"paneforge": "^0.0.5",
		"resend": "^1.1.0",
		"sharp": "^0.33.3",
		"svelte-sonner": "^0.3.24",
		"tailwind-merge": "^2.3.0",
		"tailwind-variants": "^0.2.1",
		"vaul-svelte": "^0.3.2",
		"zod": "^3.23.8"
	},
	"engines": {
		"node": ">=20.10.0"
	}
}
